{
  "name": "gateway",
  "packageManager": "yarn@4.1.1",
  "description": "oraiwasm supporting domain for all dinohub domain swarm",
  "author": "tuanto^`",
  "type": "commonjs",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "tsc --project ./tsconfig.json --outdir ./dist/cjs",
    "typeorm": "npm run build && npx typeorm -d dist/cjs/database-typeorm/datasource.js",
    "migration:generate": "npm run typeorm -- migration:generate database-typeorm/migrations/$0",
    "migration:run": "npm run build && npm run typeorm -- migration:run",
    "service:start": "NODE_PATH=dist/cjs/ node dist/cjs/app.js",
    "express-rest:dev": "concurrently \"tsc --project ./tsconfig.json --outdir dist/cjs --watch\" \"NODE_PATH=dist/cjs/ nodemon -q dist/cjs/app.js\"",
    "service:dev": "concurrently \"tsc --project ./tsconfig.json --outdir dist/cjs --watch\" \"NODE_PATH=dist/cjs/ nodemon -q dist/cjs/app.js\""
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.563.0",
    "@types/reflect-metadata": "^0.1.0",
    "amqplib": "^0.10.4",
    "axios": "^0.27.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.2",
    "env-var": "^7.4.1",
    "express": "^4.18.1",
    "express-fileupload": "^1.5.0",
    "express-validator": "^6.14.2",
    "fp-ts": "^2.16.5",
    "helmet": "^6.0.0",
    "http-status": "^1.5.2",
    "multer": "^1.4.5-lts.1",
    "nanoevents": "^9.0.0",
    "passport": "^0.6.0",
    "passport-custom": "^1.1.1",
    "passport-http-bearer": "^1.0.1",
    "passport-http-custom-bearer": "^1.0.15",
    "pg": "^8.8.0",
    "ts-pattern": "^5.1.1",
    "typeorm": "0.3.7",
    "uuid": "^8.3.2",
    "winston": "^3.8.1",
    "xss-clean": "^0.1.1",
    "yl-ddd-ts": "^2.0.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.13",
    "@types/express-fileupload": "^1",
    "@types/jest": "^29.0.0",
    "@types/multer": "^1",
    "@types/node": "^18.7.14",
    "@types/passport": "^1.0.10",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "concurrently": "^7.3.0",
    "eslint": "~8.57.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^5.0.0",
    "nodemon": "^2.0.19",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^5.4.5"
  }
}
